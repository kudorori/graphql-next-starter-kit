{
  "presets": [
    "next/babel",
    "es2015"
  ],
  "plugins": [
    "transform-object-rest-spread",
    "transform-decorators-legacy",
    "transform-class-properties",
    "transform-do-expressions",
    [
      "module-resolver",
      {
        "root": ["./client"],
        "alias": {
          "components": "/components",
          "libs": "/libs"
        }
      }
    ]
  ]
}
