{
  "name": "graphql-next-starter-kit",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon index.js --config nodemon.json",
    "build-src": "babel src --out-dir build",
    "build-front": "next build ./client/front",
    "build-admin": "next build ./client/admin",
    "start": "NODE_ENV=production node build/index.js"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "apollo-server-koa": "^1.1.2",
    "babel-core": "^6.26.0",
    "graphql": "^0.11.3",
    "graphql-merge": "0.0.1",
    "graphql-server-koa": "^1.1.2",
    "graphql-subscriptions": "^0.5.1",
    "graphql-tools": "^1.2.2",
    "import-directory": "^1.0.6",
    "kcors": "^2.2.1",
    "koa": "^2.3.0",
    "koa-body": "^2.3.0",
    "koa-passport": "^4.0.0",
    "koa-router": "^7.2.1",
    "koa-session": "^5.5.0",
    "koa-static": "^4.0.1",
    "merge-graphql-schemas": "^1.1.4",
    "mongoose": "^4.11.12",
    "mongoose2gql": "git+https://github.com/kudorori/mongoose2gql.git#gql-language",
    "next": "^3.2.2",
    "ramda": "^0.24.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "require-directory": "^2.1.1",
    "styled-components": "^2.1.2",
    "subscriptions-transport-ws": "^0.9.0"
  },
  "babel": {
    "presets": [
      "next/babel",
      "es2015"
    ],
    "plugins": [
      "transform-object-rest-spread",
      [
        "babel-plugin-inline-import",
        {
          "extensions": [
            ".graphql"
          ]
        }
      ]
    ]
  },
  "devDependencies": {
    "babel-plugin-inline-import": "^2.0.6",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1"
  }
}
